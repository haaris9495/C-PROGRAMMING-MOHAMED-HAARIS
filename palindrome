#include <stdio.h>

int main() {
    int n, temp, rev = 0;

    scanf("%d", &n);

    temp = n;   // store original number

    while (n != 0) {
        rev = rev * 10 + (n % 10);
        n = n / 10;
    }

    if (temp == rev)
        printf("Palindrome");
    else
        printf("Not Palindrome");

    return 0;
}
